<ui:UiPage x:Class="LipUI.Views.Pages.LipWebPage"
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
           xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
           xmlns:local="clr-namespace:LipUI.Views.Pages"
           d:DataContext="{d:DesignInstance local:LipWebPage, IsDesignTimeCreatable=False}"
           xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
           mc:Ignorable="d" 
           d:DesignHeight="450" d:DesignWidth="800"
           Title="LipWebPage">
    <Grid>
        <ItemsControl ItemsSource="{Binding ViewModel.AuthData}">
            <ItemsControl.ItemTemplate>
                <DataTemplate>
                    <TextBox Text="{Binding Username}"/>
                </DataTemplate>
            </ItemsControl.ItemTemplate>
        </ItemsControl>
    </Grid>
</ui:UiPage>
